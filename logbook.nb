Cell[BoxData[
 RowBox[{
  RowBox[{"critNeighbours", "[", "imgInput_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"imgData", " ", "=", " ", "imgInput"}], ",", " ", "imgCrit"}], 
     "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"imgCrit", " ", "=", " ", 
     RowBox[{"And", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"neighbours8", "[", "imgData", "]"}], " ", "\[GreaterEqual]", 
        " ", "2"}], ",", " ", 
       RowBox[{
        RowBox[{"neighbours8", "[", "imgData", "]"}], " ", "\[LessEqual]", 
        " ", "6"}]}], "]"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]",
    "]"}]}]], "Input",
 CellChangeTimes->{
  3.729967088493926*^9},ExpressionUUID->"debb76e5-225c-44e8-b1e1-\
8d69d72ea2b0"],

Cell["\<\
The critBack function checks whether the third and forth condition for the \
Zhang-Sue Algorithm deletion process are met for the current pixel. In this \
cas you can specify whether it is in the first or second alteration fo the \
Algorithm by putting in a \[OpenCurlyDoubleQuote]1\[CloseCurlyDoubleQuote] or \
a \[OpenCurlyDoubleQuote]2\[CloseCurlyDoubleQuote] as a parameter for \
version_.\
\>", "Text",
 CellChangeTimes->{{3.729967093079649*^9, 
  3.729967189482586*^9}},ExpressionUUID->"b23bc2a7-7485-40e0-b0ee-\
6c45acd03790"],

Cell[BoxData[
 RowBox[{
  RowBox[{"critBack", "[", 
   RowBox[{"imgInput_", ",", " ", "version_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"imgData", " ", "=", " ", "imgInput"}], ",", " ", "imgCrit", 
      ",", " ", 
      RowBox[{"i", "=", "2"}], ",", " ", 
      RowBox[{"j", "=", "2"}], ",", "p0", ",", " ", "p1", ",", " ", "p2", ",",
       " ", "p3", ",", " ", "p4", ",", " ", "values1", ",", " ", "values2"}], 
     "}"}], ",", " ", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"p0", " ", "=", " ", 
      RowBox[{"imgData", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", " ", "j"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p1", " ", "=", 
      RowBox[{"imgData", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", "-", "1"}], ",", " ", "j"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p2", " ", "=", " ", 
      RowBox[{"imgData", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", " ", 
         RowBox[{"j", "+", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p3", " ", "=", " ", 
      RowBox[{"imgData", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", " ", "j"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p4", " ", "=", " ", 
      RowBox[{"imgData", "[", 
       RowBox[{"[", 
        RowBox[{"i", " ", ",", " ", 
         RowBox[{"j", "-", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"version", " ", "\[Equal]", "1"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{
        RowBox[{"values1", " ", "=", "\[NonBreakingSpace]", 
         RowBox[{"{", 
          RowBox[{"p1", ",", " ", "p2", ",", " ", "p3"}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"values2", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"p2", ",", " ", "p3", ",", " ", "p4"}], "}"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"version", " ", "\[Equal]", "2"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{
        RowBox[{"values1", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"p2", ",", " ", "p1", ",", " ", "p4"}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"values2", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"p1", ",", " ", "p3", ",", " ", "p4"}], "}"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"imgCrit", " ", "=", " ", 
      RowBox[{"And", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Total", "[", "values1", "]"}], "\[GreaterEqual]", "1"}], 
        ",", " ", 
        RowBox[{
         RowBox[{"Total", "[", "values2", "]"}], "\[GreaterEqual]", " ", 
         "1"}]}], "]"}]}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{
  3.729967088493926*^9},ExpressionUUID->"972aacc8-9382-4833-943d-\
5ebeb619fee0"]
}, Closed]],

