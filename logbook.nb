          RowBox[{"[", 
           RowBox[{"i", ",", " ", 
            RowBox[{"j", "+", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
         "rule"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ne", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", "-", "1"}], ",", " ", 
            RowBox[{"j", "+", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
         "rule"}]}], " ", ";", "\[IndentingNewLine]", 
       RowBox[{"n", " ", "=", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", "-", "1"}], ",", " ", "j"}], "]"}], "]"}], "/.", " ",
          "rule"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"nw", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", " ", "-", "1"}], ",", " ", 
            RowBox[{"j", "-", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
         "rule"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"w", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{"i", " ", ",", " ", 
            RowBox[{"j", "-", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
         "rule"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sw", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", " ", "+", "1"}], ",", " ", 
            RowBox[{"j", "-", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
