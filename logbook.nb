        RowBox[{"j", "=", "2"}], ",", " ", "c", ",", " ", "e", ",", " ", "ne",
         ",", " ", "n", ",", " ", "nw", ",", " ", "w", ",", " ", "sw", ",", 
        " ", "s", ",", " ", "se", ",", "rule"}], "}"}], ",", " ", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"rule", " ", ":=", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"0", "\[Rule]", "True"}], ",", " ", 
          RowBox[{"1", "\[Rule]", " ", "False"}]}], "}"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"c", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", " ", "j"}], "]"}], "]"}], "/.", " ", "rule"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"e", " ", "=", " ", 
        RowBox[{
