             "\[IndentingNewLine]", ",", 
             RowBox[{"{", 
              RowBox[{"l", ",", " ", "1", ",", " ", "2"}], "}"}]}], "]"}], 
           ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"checkImg", "\[Equal]", " ", "imgData"}], ",", " ", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"looper", " ", "=", " ", "False"}], ";"}]}], 
            "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{"checkImg", " ", "=", " ", "imgData"}], ";"}], 
