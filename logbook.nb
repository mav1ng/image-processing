    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"D1", " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "0", ",", " ", "0", ",", " ", "1", ",", " ", "0", ",", " ", "0"}], 
         "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{
         "0", ",", " ", "0", ",", " ", "1", ",", " ", "0", ",", " ", "0"}], 
