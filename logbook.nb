        RowBox[{
         RowBox[{"i", "-", "1"}], ",", " ", 
         RowBox[{"j", "+", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p3", " ", "=", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", "-", "1"}], ",", " ", "j"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p4", " ", "=", " ", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", " ", "-", "1"}], ",", " ", 
         RowBox[{"j", "-", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p5", " ", "=", " ", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{"i", " ", ",", " ", 
         RowBox[{"j", "-", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p6", " ", "=", " ", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", " ", "+", "1"}], ",", " ", 
         RowBox[{"j", "-", "1"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p7", " ", "=", " ", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", " ", "j"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p8", " ", "=", " ", 
      RowBox[{"array", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", " ", 
