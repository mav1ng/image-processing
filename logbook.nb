       RowBox[{"s", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", "+", "1"}], ",", " ", "j"}], "]"}], "]"}], " ", "/.",
          " ", "rule"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"se", " ", "=", " ", 
        RowBox[{
         RowBox[{"imgData", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"i", "+", "1"}], ",", " ", 
            RowBox[{"j", "+", "1"}]}], "]"}], "]"}], " ", "/.", " ", 
         "rule"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"version", " ", "\[Equal]", "1"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Return", "[", 
           RowBox[{"c", " ", "&&", " ", 
            RowBox[{"!", 
             RowBox[{"(", 
              RowBox[{"n", " ", "&&", " ", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"e", " ", "&&", " ", 
