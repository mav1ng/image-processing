        RowBox[{"True", "\[Rule]", "1"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rule2", " ", ":=", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"0", "\[Rule]", "1"}], ",", " ", 
        RowBox[{"1", "\[Rule]", "0"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"values", " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{
       "p1", ",", " ", "p2", ",", " ", "p3", ",", " ", "p4", ",", " ", "p5", 
        ",", " ", "p6", ",", " ", "p7", ",", " ", "p8"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"booleanList", "=", " ", 
      RowBox[{
       RowBox[{"{", 
